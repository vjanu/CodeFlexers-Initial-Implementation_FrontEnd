<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.plusgo">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UPM.NewUserActivity"></activity>
        <activity android:name=".UserProfile" />
        <activity android:name=".TripSummaryActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UPM.VerifyCodeActivity" />
        <activity android:name=".UPM.VerifyMobilePhoneActivity" />
        <activity android:name=".Login" />
        <activity android:name=".Signup" />
        <activity android:name=".RatingMainActivity" />
        <activity android:name=".NicUploadActivity" />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>